datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
}

model Project {
  id        Int    @id @default(autoincrement())
  img       String
  title     String
  content   String
  published Boolean?  @default(true)
  createdAt DateTime? @default(now())
}
