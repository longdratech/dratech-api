datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["mongodb"]
}

model Project {
  id        String   @id @default(dbgenerated()) @map("_id") @db.ObjectId
  img       String
  title     String
  content   String
  published Boolean  @default(true)
  createdAt DateTime @default(now())
}
